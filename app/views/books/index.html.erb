<h1>Books</h1>

<h2>投稿一覧</h2>

<% @books.each do |book| %>

  <p>title</p>
  <span><%= book.title %></span>

<% end %>


<h2>New book</h2>
<!--form_with部分-->
<%= form_with model: @book do |f| %>
 <h4>title</h4>
 <%= f.text_field :title %>

 <h4>body</h4>
 <%= f.text_area :body %>

 <%= f.submit 'Create Book' %>
<% end %>